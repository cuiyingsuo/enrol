package com.cz.enrol.entity;

import javax.persistence.*;

@Table(name = "enrol_invoice")
public class Invoice {
    /**
     * 主键
     */
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    /**
     * 订单号
     */
    @Column(name = "order_no")
    private String orderNo;

    /**
     * 发票类型(0:纸质发票，1:电子发票)
     */
    private Byte type;

    /**
     * 类别(0:普票，1:专票)
     */
    private Byte category;

    /**
     * 发票号码
     */
    @Column(name = "invoice_no")
    private String invoiceNo;

    /**
     * 抬头类型(0:个人，1:单位)
     */
    @Column(name = "title_type")
    private Byte titleType;

    /**
     * 抬头
     */
    private String title;

    /**
     * 纳税人识别号
     */
    private String number;

    /**
     * 发票内容
     */
    private String content;

    /**
     * 电子发票地址
     */
    private String url;

    /**
     * 创建者
     */
    private String creator;

    /**
     * 创建时间
     */
    @Column(name = "create_time")
    private Long createTime;

    /**
     * 编辑者
     */
    private String editor;

    /**
     * 编辑时间
     */
    @Column(name = "edit_time")
    private Long editTime;

    /**
     * 获取主键
     *
     * @return id - 主键
     */
    public Long getId() {
        return id;
    }

    /**
     * 设置主键
     *
     * @param id 主键
     */
    public void setId(Long id) {
        this.id = id;
    }

    /**
     * 获取订单号
     *
     * @return order_no - 订单号
     */
    public String getOrderNo() {
        return orderNo;
    }

    /**
     * 设置订单号
     *
     * @param orderNo 订单号
     */
    public void setOrderNo(String orderNo) {
        this.orderNo = orderNo;
    }

    /**
     * 获取发票类型(0:纸质发票，1:电子发票)
     *
     * @return type - 发票类型(0:纸质发票，1:电子发票)
     */
    public Byte getType() {
        return type;
    }

    /**
     * 设置发票类型(0:纸质发票，1:电子发票)
     *
     * @param type 发票类型(0:纸质发票，1:电子发票)
     */
    public void setType(Byte type) {
        this.type = type;
    }

    /**
     * 获取类别(0:普票，1:专票)
     *
     * @return category - 类别(0:普票，1:专票)
     */
    public Byte getCategory() {
        return category;
    }

    /**
     * 设置类别(0:普票，1:专票)
     *
     * @param category 类别(0:普票，1:专票)
     */
    public void setCategory(Byte category) {
        this.category = category;
    }

    /**
     * 获取发票号码
     *
     * @return invoice_no - 发票号码
     */
    public String getInvoiceNo() {
        return invoiceNo;
    }

    /**
     * 设置发票号码
     *
     * @param invoiceNo 发票号码
     */
    public void setInvoiceNo(String invoiceNo) {
        this.invoiceNo = invoiceNo;
    }

    /**
     * 获取抬头类型(0:个人，1:单位)
     *
     * @return title_type - 抬头类型(0:个人，1:单位)
     */
    public Byte getTitleType() {
        return titleType;
    }

    /**
     * 设置抬头类型(0:个人，1:单位)
     *
     * @param titleType 抬头类型(0:个人，1:单位)
     */
    public void setTitleType(Byte titleType) {
        this.titleType = titleType;
    }

    /**
     * 获取抬头
     *
     * @return title - 抬头
     */
    public String getTitle() {
        return title;
    }

    /**
     * 设置抬头
     *
     * @param title 抬头
     */
    public void setTitle(String title) {
        this.title = title;
    }

    /**
     * 获取纳税人识别号
     *
     * @return number - 纳税人识别号
     */
    public String getNumber() {
        return number;
    }

    /**
     * 设置纳税人识别号
     *
     * @param number 纳税人识别号
     */
    public void setNumber(String number) {
        this.number = number;
    }

    /**
     * 获取发票内容
     *
     * @return content - 发票内容
     */
    public String getContent() {
        return content;
    }

    /**
     * 设置发票内容
     *
     * @param content 发票内容
     */
    public void setContent(String content) {
        this.content = content;
    }

    /**
     * 获取电子发票地址
     *
     * @return url - 电子发票地址
     */
    public String getUrl() {
        return url;
    }

    /**
     * 设置电子发票地址
     *
     * @param url 电子发票地址
     */
    public void setUrl(String url) {
        this.url = url;
    }

    /**
     * 获取创建者
     *
     * @return creator - 创建者
     */
    public String getCreator() {
        return creator;
    }

    /**
     * 设置创建者
     *
     * @param creator 创建者
     */
    public void setCreator(String creator) {
        this.creator = creator;
    }

    /**
     * 获取创建时间
     *
     * @return create_time - 创建时间
     */
    public Long getCreateTime() {
        return createTime;
    }

    /**
     * 设置创建时间
     *
     * @param createTime 创建时间
     */
    public void setCreateTime(Long createTime) {
        this.createTime = createTime;
    }

    /**
     * 获取编辑者
     *
     * @return editor - 编辑者
     */
    public String getEditor() {
        return editor;
    }

    /**
     * 设置编辑者
     *
     * @param editor 编辑者
     */
    public void setEditor(String editor) {
        this.editor = editor;
    }

    /**
     * 获取编辑时间
     *
     * @return edit_time - 编辑时间
     */
    public Long getEditTime() {
        return editTime;
    }

    /**
     * 设置编辑时间
     *
     * @param editTime 编辑时间
     */
    public void setEditTime(Long editTime) {
        this.editTime = editTime;
    }
}