package com.cz.enrol.common;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;

/**
 * @author liuzs
 * Created by liuzhongshuai on 2017/11/12.
 */
public class BaseMenu implements Serializable {

    private String code;

    private String name;

    private BaseMenu parent;

    private List<BaseMenu> childs=new ArrayList<>();

    private String url;

    private String type;

    private Integer order;



    public BaseMenu getParent() {
        return parent;
    }

    public void setParent(BaseMenu parent) {
        this.parent = parent;
    }

    public List<BaseMenu> getChilds() {
        return childs;
    }

    public void setChilds(List<BaseMenu> childs) {
        this.childs = childs;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getUrl() {
        return url;
    }

    public void setUrl(String url) {
        this.url = url;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public Integer getOrder() {
        return order;
    }

    public void setOrder(Integer order) {
        this.order = order;
    }


}
